{
  "name": "carbon-css",
  "version": "2018.0.2",
  "description": "Carbon is a simple, clean stylesheet for content.",
  "keywords": [
    "carbon",
    "css",
    "styles",
    "stylesheet",
    "stylesheets"
  ],
  "homepage": "https://github.com/skyzyx/carbon-css#readme",
  "bugs": {
